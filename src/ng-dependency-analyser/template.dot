digraph modules {
  {{~it.getModules() :module}}
    "{{= module.importName}}" [shape="box", label="{{= module.name}}"]
    {{~module.injectables :injectable}}
    "{{= injectable}}" [shape="oval"]
    {{~}}
  {{~}}

  {{~it.getModules() :module}}
    {{~module.requires :required}}
    "{{= module.importName}}" -> "{{= required }}" [label="requires"]
    {{~}}
  {{~}}

  {{~it.getModules() :module}}
    {{~module.injectables :injectable}}
    "{{= module.importName}}" -> "{{= injectable }}" [label="defines"]
    {{~}}
  {{~}}

  {{~it.getInjectedDependencies() :injectedDependency}}
  "{{= injectedDependency.filename }}" [shape="plaintext"]
  "{{= injectedDependency.injectable }}" [shape="oval"]
  {{~}}

  {{~it.getInjectedDependencies() :injectedDependency}}
  "{{= injectedDependency.injectable }}" -> "{{= injectedDependency.filename }}" [label="is used in"]
  {{~}}
}
